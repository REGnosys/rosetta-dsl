{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Rosetta DSL",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#doubleQuotedStrings"
		},
		{
			"include": "#singleQuotedStrings"
		},
		{
			"include": "#doubleQuotedDocumentation"
		},
		{
			"include": "#singleQuotedDocumentation"
		},
		{
			"include": "#numbers"
		},
		{
			"include": "#comments"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.rosetta",
				"match": "\\b(dateFormat|prefix|eligibility|False|type|when|required|output|path|qualifiedType|\\Qreal-time\\E|disjoint|else|merge|absent|join|if|reduce|item|using|in|is|optional|it|then|version|enum|basicType|contains|as|meta|\\Qone-of\\E|removeHtml|isEvent|displayName|pattern|sum|enums|default|library|min|repeatable|override|map|key|annotation|set|max|reverse|filter|single|rosettaPath|report|exists|to|choice|first|return|standard|inputs|corpus|True|source|body|empty|flatten|function|segment|only|isProduct|tag|add|all|workflow|maps|last|count|sort|docReference|ASATP|condition|extract|extends|\\Qas-key\\E|provision|hint|\\QT+2\\E|reportedField|\\QT+1\\E|\\QT+4\\E|\\QT+3\\E|\\Qassign-output\\E|\\QT+5\\E|calculationType|\\Qextract-all\\E|import|distinct|rule|mapper|\\Qonly-element\\E|synonym|and|root|regulatoryReference|alias|definition|value|reporting|lookup|or|componentID|ruleReference|recordType|multiple|any|with|func|\\Qpost-condition\\E|namespace|metaType)\\b"
			}]
		},
		"doubleQuotedStrings": {
			"name": "string.quoted.double.rosetta",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.rosetta",
					"match": "\\\\."
				}
			]
		},
		"singleQuotedStrings": {
			"name": "string.quoted.single.rosetta",
			"begin": "'",
			"end": "'",
			"patterns": [
				{
					"name": "constant.character.escape.rosetta",
					"match": "\\\\."
				}
			]
		},
		"doubleQuotedDocumentation": {
			"name": "comment.block.documentation.rosetta",
			"begin": "<\\s*\"",
			"end": "\"\\s*>"
		},
		"singleQuotedDocumentation": {
			"name": "comment.block.documentation.rosetta",
			"begin": "<\\s*'",
			"end": "'\\s*>"
		},
		"numbers": {
			"name": "constant.numeric.float.decimal.rosetta",
			"match": "[+-]?([0-9]+([.][0-9]*)?|[.][0-9]+)([Ee][+-]?[0-9]+)?"
		},
		"comments": {
			"begin": "//",
			"beginCaptures": {
				"0": {
					"name": "punctuation.definition.comment.rosetta"
				}
			},
			"end": "\\n",
			"name": "comment.line.rosetta"
		}
	},
	"scopeName": "source.rosetta"
}